{
  "name": "potfur/stash",
  "version": "v0.3",
  "description": "Stash: Mongo ODM",
  "keywords": [
    "document",
    "database",
    "persistence",
    "mongo",
    "odm"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Michal Wachowski",
      "email": "wachowski.michal@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "ext-mongo": "*",
    "ocramius/proxy-manager": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.6",
    "phpmd/phpmd": "~2.2",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-0": {"Stash\\": "./src"}
  },
  "autoload-dev": {
    "psr-0": {
      "Fake\\": "./tests",
      "Stash\\": "./tests/integration"
    }
  }
}
